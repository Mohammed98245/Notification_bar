from plyer import notification
import time
def notifyMe(title,message):
    notification.notify(
        title =title,
        message = message,
        app_icon = "C:\\Users\\LENOVO\\Downloads\\Ampeross-Qetto-2-Timer.ico",
        timeout =(10),
)





if __name__ == '__main__':
         while True:
            notifyMe("Hey User Take a Break","hello")
time.sleep(1800)
